{
  "name": "@navsnpm/katex-expression",
  "version": "1.0.1",
  "description": "A web component to render KaTeX expression",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/katex-expression/katex-expression.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "fonts": "cp -r node_modules/katex/dist/fonts dist/",
    "build": "stencil build --docs && npm run fonts",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.12.4",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "puppeteer": "1.17.0"
  },
  "license": "MIT",
  "dependencies": {
    "katex": "^0.10.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
