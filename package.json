{
  "name": "@navsnpm/katex-expression",
  "version": "1.0.8",
  "description": "A web component to render KaTeX expression",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/katex-expression/katex-expression.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prepublish": "npm run build"
  },
  "keywords": [
    "katex",
    "stenciljs",
    "stenciljs-components",
    "web-components",
    "latex"
  ],
  "homepage": "https://github.com/navsgh/katex-expression",
  "repository": {
    "type": "git",
    "url": "https://github.com/navsgh/katex-expression"
  },
  "bugs": {
    "url": "https://github.com/navsgh/katex-expression"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.12.4",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "puppeteer": "1.17.0"
  },
  "license": "MIT",
  "dependencies": {
    "katex": "^0.15.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
